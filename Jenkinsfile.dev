
pipeline {
  agent {
    kubernetes {
    label "aa-${UUID.randomUUID().toString()}"
    yamlFile 'Jenkinsfile.kubernetes.dev.yaml'
    }
  }

  environment {
    IMAGE_NAME = 'base'
    GC_REGISTRY_HOSTNAME = 'gcr.io'
    GC_PROJECT_ID_DEV = 'bc-te-dlake-dev-s7b3'
    }
  stages {
    stage('CHECKOUT') {
      steps {
        dir("${env.work}") {
          deleteDir()
          checkout scm
        }
      }
    }
      
    stage('Build Docker Image Base') {
      steps {
        container('maven'){
          script {
            dir("${env.work}"){
              sh "docker ps"
            }
          }
        }
      }
    }
    
  }
  post {
    always {
      dir("${env.work}"){
        cleanWs()
      }
    }
  }
}
